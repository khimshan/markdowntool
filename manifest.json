{
      "name": "KB URL MarkDown",
      "description": "MarkDown with Title and URL for Zendesk",
      "version": "2.62",
      "background": {
            "scripts": [
                  "browser-polyfill.js",
                  "background.js"
            ]
      },
      "browser_action": {
            "default_title": "KB URL MarkDown"
      },
      "commands": {
            "_execute_browser_action": {
                  "suggested_key": {
                        "default": "Alt+X"
                  }
            }
      },
      "permissions": [
            "tabs",
            "activeTab",
            "notifications",
            "clipboardWrite",
            "http://*/",
            "https://*/"
      ],
      "content_scripts": [
            {
                  "matches": [
                        "https://*.bittitan.com/*"
                  ],
                  "js": [
                        "browser-polyfill.js",
                        "contentscript.js"
                  ]
            }
      ],
      "manifest_version": 2,
      "web_accessible_resources": [
            "script.js"
      ]
}